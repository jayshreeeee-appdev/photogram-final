<h1> List of users </h1>

<div>

    <table border="1">
      <tr>
        <th>
          Username
        </th>

        <th>
          Private?
        </th>

        <th>
          Follow status
        </th>

        <th>
          Show details column
        </th>


        </th>
      </tr>

      <% @list_of_users.each do |a_user| %>
      <tr>
        <td>
          <%= a_user.username %>
        </td>

        <td>
          <% if a_user.private == true %>
            Yes
          <% else %>
            No
          <% end %>

        </td>

        <td>
          <%#= %> follow status
        </td>

        <td>
          <a href="/users/<%#= a_movie.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>




</div>